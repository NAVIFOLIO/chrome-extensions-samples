{
    "name" : "minimum extension",
    "version" : "1.0.0",
    "manifest_version": 3,
    "description" : "Almost minimum extension for beginner",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [{
        "matches" : ["https://www.google.com/search*"],
        "js" : [
            "content-script.js"
        ]
    }],
    "permissions" : [
        "activeTab"
    ]
}
